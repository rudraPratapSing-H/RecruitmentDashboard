}

model candidates_design {
  id                     Int      @id @default(autoincrement())
  submission_timestamp   String?  @db.Text
  email_address          String   @db.VarChar(255)
  name                   String   @db.VarChar(255)
  contact_number         String?  @db.VarChar(20)
  email_id               String?  @db.VarChar(255)
  branch                 String?  @db.VarChar(100)
  section                String?  @db.VarChar(10)
  year                   String?  @db.VarChar(10)
  vertical_1             String?  @db.VarChar(100)
  linkedin_link          String?  @db.VarChar(255)
  idea_story_1           String?  @db.Text
  idea_story_2           String?  @db.Text
  design_submission_link String?  @db.VarChar(500)
  tools_used             String?  @db.Text
  portfolio_link         String?  @db.VarChar(255)
  vertical_2             String?  @db.VarChar(100)
  introduction           String?  @db.Text
  resume_link            String?  @db.VarChar(255)
  tech_stack             String?  @db.Text
  hobbies                String?  @db.Text
  recruitment_pitch      String?  @db.Text
  status                 Boolean?
}

model ppt_submissions {
  id                     Int      @id @default(autoincrement())
  team_name              String?  @db.VarChar(500)
  idea_title             String?  @db.VarChar(1000)
  solution_description   String?  @db.Text
  presentation_file_path String?  @db.VarChar(2000)
  problem_id             String?  @db.VarChar(1000)
  status                 Boolean?
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model recruitment_round_2_2k25__responses____ops {
  email_address             String? @db.Text
  name                      String? @db.Text
  contact_number            BigInt?
  email_id                  BigInt?
  branch                    BigInt?
  section                   BigInt?
  year                      String? @db.Text
  vertical_1                String? @db.Text
  linkedin_link             String? @db.Text
  task_submission_ops       String? @db.Text
  vertical_2                String? @db.Text
  introduction              String? @db.Text
  resume_link               BigInt?
  tech_stack                BigInt?
  hobbies                   String? @db.Text
  Why_We_Should_Recruit_You String? @map("Why We Should Recruit You") @db.Text

  @@map("recruitment_round_2_2k25 (responses) - ops")
  @@ignore
}

model user {
  id       Int    @id @default(autoincrement())
  email    String @unique(map: "email") @db.VarChar(255)
  name     String @db.VarChar(255)
  password String @db.VarChar(255)
}

model interview_scores {
  id                 Int      @id @default(autoincrement())
  candidateName      String   @db.VarChar(255)
  candidateEmail     String   @db.VarChar(255)
  contactNumber      String?  @db.VarChar(20)
  vertical           String?  @db.VarChar(100)
  score              Int      @db.TinyInt
  interviewer        String   @db.VarChar(255)
  feedback           String?  @db.Text
  createdAt          DateTime @default(now())
  updatedAt          DateTime @updatedAt
  confidence         Int?
  dressing_sense     Int?
  dedication         Int?
  experience         Int?
  preferred_vertical String?  @db.VarChar(100)
  priority           Int?

  @@index([candidateEmail])
  @@index([interviewer])
}

